// Menu

IDR_MAINMENU MENU 
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&New\tCtrl+N",                IDM_NEW
        MENUITEM "&Open...\tCtrl+O",            IDM_OPEN
        MENUITEM "Open pa&rtially...\tAlt+P",   IDM_PARTIAL_OPEN
        MENUITEM SEPARATOR
        MENUITEM "&Save\tCtrl+S",               IDM_SAVE
        MENUITEM "Save &As...\tCtrl+J",         IDM_SAVE_AS
        MENUITEM "Save selec&tion as...\tCtrl+Shift+O", IDM_SAVESELAS
        MENUITEM SEPARATOR
        MENUITEM "Re&vert\tCtrl+Shift+R",       IDM_REVERT
        MENUITEM "&Insert file...\tCtrl+Shift+I", IDM_INSERTFILE
        MENUITEM "&Delete file...\tCtrl+Shift+D", IDM_DELETEFILE
        MENUITEM "Close File\tCtrl+Shift+X",    IDM_NEW
        MENUITEM SEPARATOR
        MENUITEM "&Export as hexdump...",       IDM_COPY_HEXDUMP
        MENUITEM "I&mport from hexdump...\tCtrl+Shift+H", IDM_OPEN_HEXDUMP
        MENUITEM SEPARATOR
        MENUITEM "IDM_MRU",                     IDM_MRU1
        MENUITEM "IDM_MRU",                     IDM_MRU2
        MENUITEM "IDM_MRU",                     IDM_MRU3
        MENUITEM "IDM_MRU",                     IDM_MRU4
        MENUITEM "IDM_MRU",                     IDM_MRU5
        MENUITEM "IDM_MRU",                     IDM_MRU6
        MENUITEM "IDM_MRU",                     IDM_MRU7
        MENUITEM "IDM_MRU",                     IDM_MRU8
        MENUITEM "IDM_MRU",                     IDM_MRU9
        MENUITEM SEPARATOR
        MENUITEM "E&xit\tAlt+F4",               IDM_EXIT
    END
    POPUP "&Disk"
    BEGIN
        MENUITEM "&Open drive...",              ID_DISK_OPEN_DRIVE
        MENUITEM "&Close drive",                ID_DISK_CLOSEDRIVE
        MENUITEM SEPARATOR
        MENUITEM "Goto &First Sector",          ID_DISK_GOTOFIRSTTRACK
        MENUITEM "Goto &Next Sector\tCtrl+Shift+PgDn", ID_DISK_GOTONEXTTRACK
        MENUITEM "Goto &Previous Sector\tCtrl+Shift+PgUp", 
                                                ID_DISK_GOTOPREVIOUSTRACK
        MENUITEM "Goto &Last Sector",           ID_DISK_GOTOLASTTRACK
        MENUITEM SEPARATOR
        MENUITEM "Goto &Sector #...",           ID_DISK_GOTOTRACK
    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "&Undo\tCtrl+Z",               IDM_EDIT_UNDO
        MENUITEM "&Redo\tCtrl+Y",               IDM_EDIT_REDO
        MENUITEM SEPARATOR
        MENUITEM "Cu&t...\tCtrl+X",             IDM_EDIT_CUT
        MENUITEM "&Copy...\tCtrl+C",            IDM_EDIT_COPY
        MENUITEM "&Paste...\tCtrl+V",           IDM_EDIT_PASTE
        MENUITEM "Paste &with dialog...\tCtrl+Shift+V", IDM_PASTE_WITH_DLG
        MENUITEM "&Move/Copy bytes...\tCtrl+Alt+M", IDM_EDIT_MOVE_COPY
        MENUITEM "Reverse bytes...\tCtrl+Alt+R", IDM_EDIT_REVERSE
        MENUITEM "&Append...\tCtrl+P",          IDM_EDIT_APPEND
        MENUITEM "&Delete\tDel",                IDA_DELETEKEY
        MENUITEM "&Select all\tCtrl+A",         IDM_SELECT_ALL
        MENUITEM "Select &block...\tCtrl+E",    IDM_SELECT_BLOCK
        MENUITEM "&Fill selection...\tCtrl+Shift+F", IDM_FILL_WITH
        MENUITEM SEPARATOR
        MENUITEM "Toggle h&ex/character editing\tTAB", IDM_CHANGE_MODE
        MENUITEM "Toggle keyboard I&nsert/Overwrite mode\tIns", 
                                                IDA_INSERTMODETOGGLE
        MENUITEM "Read-only mode\tAlt+R",       IDM_EDIT_READONLYMODE
        MENUITEM SEPARATOR
        POPUP "&Find and Replace"
        BEGIN
            MENUITEM "&Find...\tCtrl+F",            IDM_FIND
            MENUITEM "&Replace...\tCtrl+H",         IDM_REPLACE
            MENUITEM "Find &next\tF3",              IDM_FINDNEXT
            MENUITEM "Find &previous\tShift+F3",    IDM_FINDPREV
        END
        MENUITEM "&Go To...\tCtrl+G",           IDM_GO_TO
        MENUITEM SEPARATOR
        MENUITEM "Enter decimal &value...\tCtrl+D", 
                                                IDM_EDIT_ENTERDECIMALVALUE
        MENUITEM "Man&ipulate bits...\tCtrl+B", IDM_EDIT_MANIPULATEBITS
        MENUITEM "C&ompare from current offset...\tCtrl+M", IDM_COMPARE
        MENUITEM SEPARATOR
        MENUITEM "Get f&loating point value...\tCtrl+L", IDM_READFLOAT
        MENUITEM "File properties...\tAlt+Enter", IDM_PROPERTIES
        MENUITEM "Appl&y template...\tAlt+T",   IDM_APPLYTEMPLATE
        MENUITEM "Open in text editor\tF5",     IDM_OPEN_TEXT
    END
    POPUP "&View"
    BEGIN
        MENUITEM "Scroll &left\tCtrl+Arrow Left", IDM_SCROLL_LEFT
        MENUITEM "Scroll &right\tCtrl+Arrow Right", IDM_SCROLL_RIGHT
        MENUITEM "Scroll &up\tCtrl+Arrow Up",   IDM_SCROLL_UP
        MENUITEM "Scroll &down\tCtrl+Arrow Down", IDM_SCROLL_DOWN
        MENUITEM "Scroll up a page\tCtrl+Page Up", IDM_SCROLL_PRIOR
        MENUITEM "Scroll down a page\tCtrl+Page Down", IDM_SCROLL_NEXT
        MENUITEM SEPARATOR
        MENUITEM "Zoom in\tCtrl++", IDM_ZOOMIN
        MENUITEM "Zoom out\tCtrl+-", IDM_ZOOMOUT
        MENUITEM "Zoom normal\tCtrl+*", IDM_ZOOMNORMAL
    END
    POPUP "&Options"
    BEGIN
        MENUITEM "&View settings...\tCtrl+I",   IDM_VIEW_SETTINGS
        POPUP "&Color settings"
        BEGIN
            MENUITEM "&Text color...\tCtrl+T",      IDM_TEXT_COLOR
            MENUITEM "&Background color...\tCtrl+K", IDM_BK_COLOR
            MENUITEM "Separator color...\tCtrl+Q",  IDM_SEP_COLOR
            MENUITEM "Selection text color...\tCtrl+Shift+T", 
                                                    IDM_SELTEXT_COLOR
            MENUITEM "Selection background color...\tCtrl+Shift+K", 
                                                    IDM_SELBACK_COLOR
            MENUITEM "Bookmark color...\tCtrl+Shift+B", IDM_BMK_COLOR
            MENUITEM SEPARATOR
            MENUITEM "Reset colors to standard...\tCtrl+Shift+S", 
                                                    IDM_RESET_COLORS
            MENUITEM "&Adopt OS colour scheme...\tCtrl+Shift+A", 
                                                    IDM_ADOPT_COLOURS
        END
        MENUITEM "C&haracter set...\tCtrl+R",   IDM_CHARACTER_SET
        MENUITEM "B&inary mode...\tCtrl+Shift+Y", IDM_BINARYMODE
        MENUITEM "&OLE Drag-drop...\tCtrl+Alt+O", IDM_OLE_DRAG_DROP
        MENUITEM "Make backups\tCtrl+Shift+M",  IDM_MAKE_BACKUPS, CHECKED
    END
    POPUP "&Registry"
    BEGIN
        MENUITEM "In &all context menu",        IDM_CONTEXT, CHECKED
        MENUITEM "In &unknown context menu",    IDM_UNKNOWN, CHECKED
        MENUITEM "Is &default in unknown context", IDM_DEFAULT, CHECKED
        MENUITEM "&Save ini when required",     IDM_SAVEINI, CHECKED
        MENUITEM "Change &instance...",         IDM_CHANGEINST
        MENUITEM "S&hortcuts to Frhed...",      IDM_SHORTCUTS
    END
    POPUP "&Bookmarks"
    BEGIN
        MENUITEM "&Add bookmark...\tCtrl+W",    IDM_ADDBOOKMARK
        MENUITEM "&Remove bookmark...\tAlt+W",  IDM_REMOVE_BKM
        MENUITEM "&Clear all bookmarks\tCtrl+Shift+W", IDM_CLEARALL_BMK
        MENUITEM SEPARATOR
        MENUITEM "bmk",                         IDM_BOOKMARK1
        MENUITEM "bmk",                         IDM_BOOKMARK2
        MENUITEM "bmk",                         IDM_BOOKMARK3
        MENUITEM "bmk",                         IDM_BOOKMARK4
        MENUITEM "bmk",                         IDM_BOOKMARK5
        MENUITEM "bmk",                         IDM_BOOKMARK6
        MENUITEM "bmk",                         IDM_BOOKMARK7
        MENUITEM "bmk",                         IDM_BOOKMARK8
        MENUITEM "bmk",                         IDM_BOOKMARK9
    END
    POPUP "&Misc"
    BEGIN
        MENUITEM "Goto DLL e&xport names...",   ID_MISC_GOTO_DLL_EXPORTS
        MENUITEM "Goto DLL import names...",    ID_MISC_GOTO_DLL_IMPORTS
        MENUITEM SEPARATOR
        MENUITEM "Encode / Decode...",          ID_MISC_ENCODEDECODE
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&Help topics...\tF1",         IDM_HELP_TOPICS
        MENUITEM SEPARATOR
        MENUITEM "&About Frhed...",             IDM_ABOUT
    END
END

IDR_CONTEXTMENU MENU 
BEGIN
    POPUP "0"
    BEGIN
        MENUITEM "Cu&t...\tCtrl+X",             IDM_EDIT_CUT
        MENUITEM "&Copy...\tCtrl+C",            IDM_EDIT_COPY
        MENUITEM "&Paste...\tCtrl+V",           IDM_EDIT_PASTE
        MENUITEM "&Delete\tDel",                IDA_DELETEKEY
        MENUITEM SEPARATOR
        MENUITEM "&Find...\tCtrl+F",            IDM_FIND
        MENUITEM "Replace...\tCtrl+H",          IDM_REPLACE
        MENUITEM "Find next\tF3",               IDM_FINDNEXT
        MENUITEM "Find previous\tShift+F3",     IDM_FINDPREV
        MENUITEM "&Go To...\tCtrl+G",           IDM_GO_TO
    END
    POPUP "1"
    BEGIN
        MENUITEM "Goto &First Sector",          ID_DISK_GOTOFIRSTTRACK
        MENUITEM "Goto &Next Sector",           ID_DISK_GOTONEXTTRACK
        MENUITEM "Goto &Previous Sector",       ID_DISK_GOTOPREVIOUSTRACK
        MENUITEM "Goto &Last Sector",           ID_DISK_GOTOLASTTRACK
        MENUITEM SEPARATOR
        MENUITEM "Goto &Sector #...",           ID_DISK_GOTOTRACK
    END
END

